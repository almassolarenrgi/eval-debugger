{
	"inherit": "vshaxe-node",
	"targets": [
		{
			"name": "eval-debugger",
			"afterBuildCommands": [
				["npx", "webpack", "--mode", "production", "--silent"]
			],
			"args": {
				"classPaths": [
					"src"
				],
				"haxelibs": [
					"vscode-debugadapter"
				],
				"output": {
					"path": "bin/adapter.js",
					"target": "js"
				},
				"main": "Main"
			}
		}
	]
}